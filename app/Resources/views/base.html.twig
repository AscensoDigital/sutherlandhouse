{% spaceless %}
<!DOCTYPE html>
<html lang="es">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}{% endblock title %} - {% block subtitle %}{% endblock subtitle %} - Hotel Sutherland House - Hotel Boutique - Valparaíso</title>
        <link rel="stylesheet" href="{{ asset('css/vendor/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/vendor/bootstrap-theme.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/vendor/font-awesome.min.css') }}">
        {% block stylesheets %}
            {% stylesheets
                'css/main.css'
                filter='scssphp' output='css/compiled/index.css' %}
            <link href="{{ asset_url }}" rel="stylesheet" type="text/css"/>
            {% endstylesheets %}
        {% endblock stylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="wrap">
            <nav id="nav-header" class="navbar navbar-default navbar-static-top" role="navigation">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#ts-navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="clean-link" href="{{ path('homepage') }}">
                        <img id="logo" class="img-responsive" src="{{ asset('images/hotel-sutherland-house-logo-horizontal.png') }}" alt="Logo Hotel Sutherland House Valparaíso"/>
                    </a>
                </div>
                <div class="collapse navbar-collapse navbar-ex1-collapse" id="ts-navbar-collapse">
                    <div id="nav-contact" class="col-sm-5 col-md-6 col-lg-7 pull-right text-right">
                        <div id="google_translate_element" class="navbar-form pull-right"></div>
                        <span class="navbar-text pull-right"><i class="fa fa-phone"></i> +56 32 3196 798</span>
                        <span class="navbar-text pull-right"><i class="fa fa-envelope-open-o"></i> contacto<i class="fa fa-at"></i>sutherlandhouse.cl</span>
                    </div>
                    <ul class="nav navbar-nav navbar-right col-">
                        {{ include('includes/menu-principal.html.twig') }}
                        <li>&nbsp;</li>
                    </ul>
                </div>
            </nav>
            <div id="main" class="container-fluid">
            {% block body %}{% endblock body %}
            </div>
        </div>
        <footer id="footer" class="panel-footer panel-primary">
            <div class="row">
                <div class="col-md-4 text-left">
                    <img src="{{ asset('images/ddsecure_140x40_orange.png') }}" alt="Hotel Sutherland House Secure">
                </div>
                <div class="col-md-4 text-center">
                    <a href="waze://?q=Hotel+Sutherland+House"><i class="fa fa-map-marker"></i> Avenida Alemania 4966, Cerro Alegre, Valparaíso, Chile</a><br>
                    <span><i class="fa fa-phone"></i> +56 32 3196 798</span> - <a href="mailto:contacto@sutherlandhouse.cl?subject=%5BWEB%5D+Contacto"><i class="fa fa-envelope-open-o"></i> contacto<i class="fa fa-at"></i>sutherlandhouse.cl</a>
                </div>
                <div class="col-md-4 text-right">
                    <a href="https://www.facebook.com/hotel.sutherland.house/" target="_blank"><i class="fa fa-2x fa-facebook-official"></i></a>&nbsp;
                    <a href="https://www.tripadvisor.cl/Hotel_Review-g294306-d1957998-Reviews-Hotel_Boutique_Sutherland_House-Valparaiso_Valparaiso_Region.html" target="_blank"><i class="fa fa-2x fa-tripadvisor"></i></a>&nbsp;
                    <a href="https://plus.google.com/+SutherlandhouseCl" target="_blank"><i class="fa fa-2x fa-google-plus-square"></i></a>
                </div>

            </div>
        </footer>
        <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
        {% block javascripts %}
            {% javascripts
            'js/vendor/bootstrap.min.js'
            filter='?jsqueeze' output='js/compiled/base.js' %}
            <script src="{{ asset_url }}" type="text/javascript"></script>
            {% endjavascripts %}
        {% endblock %}
        <script type='text/javascript'>
            var paxer = {};
            paxer.secret = 'JFL9VF';
            paxer.widgetType = 'BOTH';
            paxer.triggerButton = '';  // Coloque aquí el id del botón o link de reservas de su sitio web
            paxer.widgetTheme = 'custom';
            paxer.widgetOrientation = 'horizontal';
            paxer.widgetColors = {"colorHeader1":"#c2bc00","colorHeader2":"#3b3a3a","colorBody1":"#f4f4f4","colorBody2":"#666666"};
            paxer.paxerLang = ''; // Opcional: indique el código del idioma en el que desea ver el widget. Si no se especifica, establece el idioma principal del hotel. Idiomas soportados: es, en

            (function() {
                var paxerScript = document.createElement('script');
                paxerScript.type = 'text/javascript';
                paxerScript.async = true;
                paxerScript.src = 'https://paxer.com/lib/widget/paxer-booking.js?v='+ (new Date().getTime());
                var hscr = document.getElementsByTagName('head')[0];
                hscr.appendChild(paxerScript);
            })();
        </script>
        {% if app.environment=='prod' %}
            <script>
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
                ga('create', 'UA-16729802-1', 'auto');
                ga('send', 'pageview');
            </script>
        {% endif %}
        <script type="text/javascript">
            function googleTranslateElementInit() {
                new google.translate.TranslateElement({pageLanguage: 'es', includedLanguages: 'ca,da,de,en,fr,hr,it,ja,ko,no,pt,ru,sk,sv,zh-TW', multilanguagePage: true, gaTrack: true, gaId: 'UA-16729802-1'}, 'google_translate_element');
            }
        </script><script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
    </body>
</html>
{% endspaceless %}